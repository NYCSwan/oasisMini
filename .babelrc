{
  "compact": false,
  "plugins": [
    "react-hot-loader/babel",
    "transform-class-properties"
  ],
  "env": {
    "server": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  },
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions"
          ]
        },
        "loose": true,
        "modules": false
      }
    ]
  ]
}
